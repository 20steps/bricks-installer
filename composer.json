{
    "name": "20steps/bricks-installer",
    "description": "The official installer to create projects based on the Bricks platform by 20steps.",
    "keywords": ["bricks", "platform", "installer", "php"],
    "homepage": "https://github.com/20steps/bricks-installer",
    "license": "MIT",
    "authors": [
        {
            "name": "Symfony Community",
            "homepage": "http://symfony.com/contributors"
        },
        {
            "name": "Helmut Hoffer von Ankershoffen (hhva@20steps.de)",
            "homepage": "http://20steps.de"
        }
    ],
    "autoload": {
        "psr-4": { "Bricks\\": "src/Bricks/" }
    },
    "require": {
        "php": ">=7.0.0",
        "guzzlehttp/guzzle": "^6.0",
        "symfony/console": "^3.0",
        "symfony/filesystem": "^3",
        "raulfraile/distill": "~0.9,!=0.9.3,!=0.9.4"
    },
    "require-dev": {
        "symfony/process": "^3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "platform": {
            "php": "7.0.0"
        }
    },
    "bin": [
        "bricks"
    ]
}
