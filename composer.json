{
    "name": "bricks/bricks-installer",
    "description": "The official installer to create projects based on the Bricks platform.",
    "keywords": ["bricks", "platform", "installer", "php"],
    "license": "MIT",
    "authors": [
        {
            "name": "Symfony Community",
            "homepage": "http://symfony.com/contributors"
        },
        {
            "name": "Helmut Hoffer von Ankershoffen (hhva@20steps.de)",
            "homepage": "http://20steps.de"
        }
    ],
    "autoload": {
        "psr-4": { "Bricks\\": "src/Bricks/" }
    },
    "require": {
        "php": ">=7.0.0",
        "guzzlehttp/guzzle": "^5.3.1",
        "symfony/console": "~2.6",
        "symfony/filesystem": "~2.5",
        "raulfraile/distill": "~0.9,!=0.9.3,!=0.9.4"
    },
    "require-dev": {
        "symfony/process": "~2.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "bin": [
        "bricks"
    ]
}
